  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kirbym/devconnect/issues"
  },
  "homepage": "https://github.com/kirbym/devconnect#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.5.1",
    "dot-prop": "^6.0.1",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "gravatar": "^1.8.2",
    "hosted-git-info": "^4.0.2",
    "ini": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^6.1.3",
    "mquery": "^4.0.0",
    "normalize-package-data": "^3.0.3",
    "npm": "^8.3.0",
    "read-pkg": "^7.0.0",
