  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kirbym/devconnect/issues"
  },
  "homepage": "https://github.com/kirbym/devconnect#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.0.2",
    "dot-prop": "^6.0.1",
    "dotenv": "^8.6.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "gravatar": "^1.8.1",
    "ini": "^2.0.0",
    "jsonwebtoken": "^8.4.0",
    "moment": "^2.29.1",
    "mongoose": "^5.12.7",
    "mquery": "^3.2.5",
    "npm": "^7.12.0",
    "request": "^2.88.0",
